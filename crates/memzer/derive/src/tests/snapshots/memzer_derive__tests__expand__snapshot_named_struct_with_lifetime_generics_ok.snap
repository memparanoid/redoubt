impl<'alpha, Tau> memzer_core::ZeroizeMetadata for Sigma<'alpha, Tau>
where
    Tau: Clone,
{
    const CAN_BE_BULK_ZEROIZED: bool = false;
}
impl<'alpha, Tau> memzer_core::FastZeroizable for Sigma<'alpha, Tau>
where
    Tau: Clone,
{
    fn fast_zeroize(&mut self) {
        let fields: [&mut dyn memzer_core::FastZeroizable; 4] = [
            memzer_core::collections::to_fast_zeroizable_dyn_mut(&mut self.alpha),
            memzer_core::collections::to_fast_zeroizable_dyn_mut(&mut self.beta),
            memzer_core::collections::to_fast_zeroizable_dyn_mut(self.gamma),
            memzer_core::collections::to_fast_zeroizable_dyn_mut(&mut self.__sentinel),
        ];
        memzer_core::collections::zeroize_collection(&mut fields.into_iter())
    }
}
impl<'alpha, Tau> memzer_core::ZeroizationProbe for Sigma<'alpha, Tau>
where
    Tau: Clone,
{
    fn is_zeroized(&self) -> bool {
        let fields: [&dyn memzer_core::ZeroizationProbe; 3] = [
            memzer_core::collections::to_zeroization_probe_dyn_ref(&self.alpha),
            memzer_core::collections::to_zeroization_probe_dyn_ref(&self.beta),
            memzer_core::collections::to_zeroization_probe_dyn_ref(self.gamma),
        ];
        memzer_core::collections::collection_zeroed(&mut fields.into_iter())
    }
}
impl<'alpha, Tau> memzer_core::AssertZeroizeOnDrop for Sigma<'alpha, Tau>
where
    Tau: Clone,
{
    fn clone_sentinel(&self) -> memzer_core::ZeroizeOnDropSentinel {
        self.__sentinel.clone()
    }
    fn assert_zeroize_on_drop(self) {
        memzer_core::assert::assert_zeroize_on_drop(self);
    }
}
